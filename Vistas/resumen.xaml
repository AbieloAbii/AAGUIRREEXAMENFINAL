<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="aaguirreexamen.Vistas.resumen"
             Title="resumen">
    <VerticalStackLayout Spacing="20" Padding="50">
        <Label x:Name="lblUsuario" FontSize="18" HorizontalTextAlignment="End" TextColor="Red"></Label>
        <Label 
            Text="RESUMEN"
            FontSize="22"
            TextColor="Red"
            HorizontalOptions="Center" />
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            
        </Grid>


    </VerticalStackLayout>
</ContentPage>